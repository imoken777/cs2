    "import math\n",
    "\n",
    "\n",
    "def entropy(lst):\n",
    "    return -sum(x * math.log2(x) for x in lst)\n",
    "\n",
    "\n",
    "def check(lst):\n",
    "    result = 0\n",
    "    for i in lst:\n",
    "        result += i\n",
    "    return result\n",
    "\n",
    "\n",
    "lst = [1 / 10, 1 / 10, 1 / 10, 1 / 10, 3 / 10, 3 / 10]\n",
    "print(check(lst))\n",
    "print(entropy(lst))"
   ]
  }
